{
	"name": "three-start",
	"version": "1.0.0",
	"description": "Boiler Plate for compiling three js with webpack",
	"main": "index.js",
	"scripts": {
		"//": "// nodemon watches the webpack config to automatically update when wds is running",
		"watch": "webpack -w",
		"serve": "webpack-dev-server --env development",
		"build": "webpack --env production",
		"serve-prod": "webpack-dev-server --env production",
		"serve-host": "webpack-dev-server --env development --port 8030 --host 192.168.1.17",
		"nodemon": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env development\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kyehuelin/three-start.git"
	},
	"keywords": [
		"three.js",
		"webpack"
	],
	"author": "Kye Huelin",
	"license": "ISC",
	"homepage": "https://github.com/kyehuelin/three-starter#readme",
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-es2015-destructuring": "^6.23.0",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"bootstrap": "^3.3.7",
		"copy-webpack-plugin": "^4.2.3",
		"css-loader": "^0.28.7",
		"eslint": "^4.12.1",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.5",
		"html-webpack-include-assets-plugin": "^1.0.2",
		"html-webpack-plugin": "^2.30.1",
		"image-webpack-loader": "^3.4.2",
		"jquery": "^3.2.1",
		"node-sass": "^4.7.2",
		"nodemon": "^1.12.1",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.0",
		"traverse": "^0.6.6",
		"url-loader": "^0.6.2",
		"webpack": "^3.8.1",
		"webpack-dev-server": "^2.9.5",
		"webpack-merge": "^4.1.1"
	},
